{
  "version": 2,
  "builds": [
    {
      "src": "src/client/dist/**",
      "use": "@vercel/static"
    },
    {
      "src": "src/server/working-server.ts",
      "use": "@vercel/node",
      "config": {
        "maxDuration": 30
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/src/server/working-server.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/src/client/dist/$1"
    }
  ]
}