{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist/client",
  "functions": {
    "src/server/working-server.ts": {
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/src/server/working-server.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/client/index.html"
    }
  ]
}